@model IEnumerable<HalloDoc.DataModels.RequestBusiness>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ip)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Business)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Request)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Business.BusinessId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Request.RequestId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.RequestBusinessId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.RequestBusinessId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.RequestBusinessId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
